{
  "name": "react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "oidc-client-ts": "^3.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-oidc-context": "^3.3.0",
    "react-router": "^7.8.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.x",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": false,
    "semi": true,
    "bracketSameLine": true,
    "bracketSpacing": true,
    "vueIndentScriptAndStyle": true,
    "trailingComma": "all",
    "singleAttributePerLine": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "not dead"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
