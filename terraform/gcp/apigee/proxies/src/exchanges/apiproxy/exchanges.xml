<APIProxy name="exchanges">
    <DisplayName>Exchange rates API</DisplayName>
    <Description>Expose average exchange rates from Polish National Bank</Description>
    <Policies>
        <Policy>add-cors</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
    </ProxyEndpoints>
    <TargetEndpoints>
        <TargetEndpoint>default</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
