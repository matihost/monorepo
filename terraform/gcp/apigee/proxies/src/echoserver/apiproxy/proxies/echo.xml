<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="echo">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response>
      <Step>
        <Name>AM-AddOriginalBasePathHeader</Name>
      </Step>
    </Response>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/echo</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
