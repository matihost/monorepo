<APIProxy name="echoserver">
    <DisplayName>Echoserver API</DisplayName>
    <Description>Echoserver API</Description>
    <Policies>
        <Policy>add-cors</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>echo</ProxyEndpoint>
        <ProxyEndpoint>echoserver</ProxyEndpoint>
    </ProxyEndpoints>
    <TargetEndpoints>
        <TargetEndpoint>default</TargetEndpoint>
    </TargetEndpoints>
</APIProxy>
